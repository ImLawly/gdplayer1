<div class="row">
    <div class="col-12">
        <h1 class="h4 mb-3">{{ title }}</h1>
        <div class="toolbar d-flex flex-wrap toolbar mb-3">
            <button type="button" class="btn btn-danger btn-sm btn-hidden d-none" onclick="sessions.delete.multi()">
                <em class="fas fa-trash-alt mr-2"></em>Delete
            </button>
            <button type="button" class="btn btn-info btn-sm" onclick="sessions.reload()">
                <em class="fas fa-sync-alt mr-2"></em>Reload
            </button>
        </div>
        <table id="tbSessions" class="table table-striped table-bordered table-hover table-sm m-0" style="width:100%" aria-describedby="User Session List">
            <thead>
                <tr>
                    <th scope="col" style="width:64px!important">
                        <div class="custom-control custom-checkbox mx-auto">
                            <input type="checkbox" class="custom-control-input" id="ckAllSessions" data-tableid="#tbSessions">
                            <label class="custom-control-label" for="ckAllSessions"></label>
                        </div>
                    </th>
                    <th scope="col">Username</th>
                    <th scope="col">IP</th>
                    <th scope="col">UA</th>
                    <th scope="col">Added</th>
                    <th scope="col">Expires</th>
                    <th scope="col">Actions</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th scope="col" style="width:64px!important">
                        <div class="custom-control custom-checkbox mx-auto">
                            <input type="checkbox" class="custom-control-input" id="ckAllSessions1">
                            <label class="custom-control-label" for="ckAllSessions1"></label>
                        </div>
                    </th>
                    <th scope="col">Username</th>
                    <th scope="col">IP</th>
                    <th scope="col">UA</th>
                    <th scope="col">Added</th>
                    <th scope="col">Expires</th>
                    <th scope="col">Actions</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
